import math
import time
start=time.time()
def asal_mi(n):
    if n % 2 == 0 and n > 2: 
        return False
    return all(n % i for i in range(3, int(math.sqrt(n)) + 1, 2))

def reverse(num):
    a=str(num)
    return int(a[::-1])
  
b=[i for i in range(2,1000000) if asal_mi(i) and asal_mi(reverse(i))]

print(b)

print("it tooks ",(time.time()-start))
